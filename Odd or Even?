// https://www.codewars.com/kata/5949481f86420f59480000e7

S1
function oddOrEven(arr) {
   return arr.reduce((a,b) => a + b, 0) % 2 ? 'odd' : 'even';
}

S2
function oddOrEven(arr) {
  let sum = 0;
  for ( let i = 0; i < arr.length; i++){
  sum += arr[i];
  }
  if(sum % 2 === 0) return 'even';
  else return 'odd';
}
