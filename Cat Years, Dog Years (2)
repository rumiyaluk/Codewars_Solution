var ownedCatAndDog = function(catYears, dogYears) {
  let cat = (catYears < 24)? catYears /15 : (catYears - 16)/ 4;
  let dog = (dogYears < 24)? dogYears /15 :(dogYears - 14)/ 5;
return [ Math.floor (cat), Math.floor(dog)];
  }
