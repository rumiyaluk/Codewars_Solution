// https://www.codewars.com/kata/5899dc03bc95b1bf1b0000ad

1S
function invert(array) {
   return array.map(x => x * (-1));
}

2S
function invert(array) {
   let newArr = [];
  for (let i = 0; i < array.length; i++){
  if(array [i] === 0){
    newArr.push(0);
  }else {
    let temp = array [i] * -1;
    newArr.push(temp);
    }
  }
 return newArr;
}
